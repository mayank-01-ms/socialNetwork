<h1>Invites</h1>
<% @invites.each do | invite | %> 
    <div style="margin-top: 1rem">
        <%= render "profile", obj: invite.user %>
        <%= button_to "Accept", accept_invite_path, params: {friend: invite.user} %>
        <%= button_to "Deny", deny_invite_path, method: :delete, params: {friend: invite.user} %>
    </div> 
<% end %>